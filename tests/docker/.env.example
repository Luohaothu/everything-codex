CODEX_API_KEY=your-codex-api-key-here
OPENAI_BASE_URL=https://your-openai-base-url.example.com/v1
CODEX_TEST_MODEL=gpt-5.3-codex
CODEX_TEST_REASONING_EFFORT=medium
CODEX_TEST_TIMEOUT=180
# Keep CODEX_VERSION in sync with:
# - tests/docker/Dockerfile (ARG CODEX_VERSION default)
# - tests/docker/docker-compose.yml (build.args.CODEX_VERSION default)
CODEX_VERSION=0.98.0
# For local Docker usage, set these to your host IDs (`id -u` / `id -g`)
# when running docker compose manually, so report mounts stay writable.
# run-layers.sh defaults to host IDs automatically when unset in shell env.
TESTUSER_UID=1001
TESTUSER_GID=1001
